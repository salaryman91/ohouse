# 안드로이드 디바이스 / 에뮬레이터 UDID
# - adb devices 로 확인한 값 사용
UDID=

# 오늘의집 정상 로그인용 계정 정보
# - 실제 과제 제출 시에는 비워두거나, 안전한 테스트 계정 사용 권장
OH_EMAIL=
OH_PASSWORD=

# 앱 초기화 플래그
# - true  : 테스트 시작 전에 백그라운드 앱 정리 + 오늘의집 force-stop + pm clear
# - false : 앱 상태를 유지한 채로 테스트 (디버깅용)
RESET_APP=true

# 로그인 실패 시나리오용 계정 정보
# - 실제 계정 잠금 방지를 위해, 전용 테스트 계정/비밀번호 사용 권장
FAIL_EMAIL=
FAIL_PASSWORD=